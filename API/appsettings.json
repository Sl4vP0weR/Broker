{
  "ApplicationSettings": {
    "DefaultBase": "USD",
    "AvailablePeriodDays": 60,
    "SupportedCurrencies": [
      "RUB",
      "EUR",
      "GBP",
      "JPY"
    ]
  },
  "LoggingSettings": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Information",
      "Overide": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "/logs/log-.txt",
          "rollingInterval": "Day",
          "rollOnFileSizeLimit": true
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  },
  "RedisSettings": {
    "ConnectionString": "localhost:5002"
  },
  "AllowedHosts": "*"
}
